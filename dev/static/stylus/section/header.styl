// hovers
.header__btn {
  transition: $transition;
  &:hover {
    background-color: rgb(25, 146, 233);
  }
}
.contact-us__btn,
.bid__form-submit {
  transition: $transition;
  &:hover {
    box-shadow: 0 0 15px rgb(245, 239, 163);
  }
}
.step__links a,
.address__transit {
  transition: $transition;
  &.bordered_link:hover {
    border-color: #ddea38;
  }
  &:hover {
    color: $yellow;
  }
}
.license__certificate a:after {
  content: "";
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 50px;
